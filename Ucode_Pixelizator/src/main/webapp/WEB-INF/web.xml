<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="css/style.css"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>corrupted</servlet-name>
        <servlet-class>world.ucode.Corrupted</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>pixel</servlet-name>
        <servlet-class>world.ucode.Pixelate</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>corrupted</servlet-name>
        <url-pattern>/corrupted</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>pixel</servlet-name>
        <url-pattern>/pixel</url-pattern>
    </servlet-mapping>
</web-app>